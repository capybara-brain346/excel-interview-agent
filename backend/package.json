{
    "name": "excel-interview-agent-backend",
    "version": "1.0.0",
    "description": "Express backend for Excel Interview Agent",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "nodemon src/index.ts",
        "build": "tsc",
        "build:watch": "tsc --watch",
        "typeorm": "typeorm-ts-node-commonjs",
        "db:migrate": "npm run typeorm migration:run -- -d src/config/database.ts",
        "db:rollback": "npm run typeorm migration:revert -- -d src/config/database.ts",
        "db:generate": "npm run typeorm migration:generate -- -d src/config/database.ts",
        "db:create": "npm run typeorm migration:create",
        "db:seed": "ts-node src/seeds/index.ts",
        "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed",
        "logs:clear": "rm -rf logs/*.log",
        "test": "jest",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix"
    },
    "keywords": [
        "express",
        "nodejs",
        "backend",
        "typescript",
        "postgresql"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.18.2",
        "express-rate-limit": "^6.10.0",
        "express-validator": "^7.0.1",
        "helmet": "^7.0.0",
        "joi": "^17.9.2",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.17",
        "uuid": "^9.0.0",
        "winston": "^3.10.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.4",
        "@types/node": "^20.5.0",
        "@types/pg": "^8.10.2",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "eslint": "^8.47.0",
        "jest": "^29.6.2",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2"
    }
}
